  head
    meta(charset='utf-8')
    block title(pageTitle)
      title=pageTitle
    meta(name='description', content='')
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    
    block style
    link(rel='stylesheet', href='css/style.css')
    link(rel='stylesheet', href='//netdna.bootstrapcdn.com/font-awesome/3.2.1/css/font-awesome.css')

  body
    block upper
      #top
        .top-nav
          .nav-wrap.row
            header.col-md-4.col-sm-4.col-xs-4
              .logo(alt='Tok3n')
            nav.col-md-8.col-sm-8.col-xs-8
              .menu
                block navlist
                  ul
                    li.developers
                      a(href='#') Developers
                    li
                      button.btn.btn-default(href='#') Log out
                    li.toggle-sidebar
                      button#collapseSidebarButton.btn.btn-default
                        i.icon-reorder
    block layout
      #layout
        block body
          block nav
            #sidebar
              .nav-inner
                ul.menu#sidebarMenu.collapsed
                  li#devicesTrigger.col-xs-12.col-sm-12.col-md-12
                    .row
                      .icon.col-xs-2.col-sm-3.col-sm-offset-1.col-md-3.col-md-offset-1
                        i.icon-mobile-phone
                      .title.col-xs-10.col-sm-7.col-md-7 My devices
                  li.col-xs-12.col-sm-12.col-md-12
                    .row
                      .icon.col-xs-2.col-sm-3.col-sm-offset-1.col-md-3.col-md-offset-1
                        i.icon-phone
                      .title.col-xs-10.col-sm-7.col-md-7 My phone lines
                  li#myDomainsTrigger.col-xs-12.col-sm-12.col-md-12
                    .row
                      .icon.col-xs-2.col-sm-3.col-sm-offset-1.col-md-3.col-md-offset-1
                        i.icon-lock
                      .title.col-xs-10.col-sm-7.col-md-7 Authorized apps
                  li#integrationsTrigger.col-xs-12.col-sm-12.col-md-12
                    .row
                      .icon.col-xs-2.col-sm-3.col-sm-offset-1.col-md-3.col-md-offset-1
                        i.icon-code
                      .title.col-xs-10.col-sm-7.col-md-7 My integrations
                  li.col-xs-12.col-sm-12.col-md-12
                    .row
                      .icon.col-xs-2.col-sm-3.col-sm-offset-1.col-md-3.col-md-offset-1
                        i.icon-cogs
                      .title.col-xs-10.col-sm-7.col-md-7 Settings
    block footer

